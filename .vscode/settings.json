{
  "search.exclude": {
    "node_modules": true,
  },
  "files.eol": "\n",
  "editor.tabSize": 2,
  "iconify.inplace": false,
  "less.lint.unknownAtRules": "ignore",

  // format on save
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "cSpell.words": [
    "Abci",
    "antfu",
    "namada",
    "VITE"
  ]
}
